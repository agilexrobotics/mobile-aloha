<launch>
    <!-- unique camera name-->
    <arg name="camera_name" default="camera"/>
    <!-- Hardware depth registration -->
    <arg name="3d_sensor" default="dabai_u3"/>
    <!-- stereo_s_u3, astrapro, astra -->
    <arg name="device_num" default="5"/>
    
    <arg name="camera1_prefix" default="1"/>
    <arg name="camera2_prefix" default="2"/>
    <arg name="camera3_prefix" default="3"/>
    <arg name="camera4_prefix" default="4"/>
    <arg name="camera5_prefix" default="5"/>
    
    <arg name="camera1_serila_number" default="AU1953304MG"/>
    <arg name="camera2_serila_number" default="AU1L12100AZ"/>
    <arg name="camera3_serila_number" default="AU1P32201SA"/>

    <include file="$(find astra_camera)/launch/$(arg 3d_sensor).launch">
        <arg name="camera_name" value="$(arg camera_name)$(arg camera1_prefix)"/>
        <arg name="serial_number" value="$(arg camera1_serila_number)"/>
        <arg name="device_num" value="$(arg device_num)"/>

    </include>

    <include file="$(find astra_camera)/launch/$(arg 3d_sensor).launch">
        <arg name="camera_name" value="$(arg camera_name)$(arg camera2_prefix)"/>
        <arg name="serial_number" value="$(arg camera2_serila_number)"/>
        <arg name="device_num" value="$(arg device_num)"/>
    </include>

     <include file="$(find astra_camera)/launch/$(arg 3d_sensor).launch">
        <arg name="camera_name" value="$(arg camera_name)$(arg camera3_prefix)"/>
        <arg name="serial_number" value="$(arg camera3_serila_number)"/>
        <arg name="device_num" value="$(arg device_num)"/>
    </include>

</launch>
